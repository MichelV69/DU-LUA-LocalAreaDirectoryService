{
  "cli": {
    "fmtVersion": 3
  },
  "name": "Starlantis-YouAreHere-AMD",
  "description": "Distributed Directory Service for Starlantis Station",
  "sourcePath": "src",
  "outputPath": "out",
  "libs": [],
  "builds": {
    "main": {
      "name": "main",
      "type": "control",
      "slots": {
       "DB": {
          "name": "DB",
          "type": "databank"
        },
        "Display": {
          "name": "Display",
          "type": "screen"
        },
        "Receiver": {
          "name": "Receiver",
          "type": "receiver"
        },
        "Emitter": {
          "name": "Emitter",
          "type": "emitter"
        }
      }
    }
  },
  "targets": {
    "development": {
      "name": "development",
      "minify": false,
      "handleErrors": false
    }
  },
  "internalPaths": [
    "autoconf/",
    "cpml/",
    "pl/",
    "utils/event"
  ]
}